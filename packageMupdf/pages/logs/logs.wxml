<!--logs.wxml-->
<view class="container" style="background:#ededf0">
    <image wx:if="{{array.length > 0}}" wx:for="{{array}}" style="margin-bottom:2px;margin-top:2px;width: {{item.width}}px;height:{{item.height}}px;"
           mode="aspectFit"
           bindtap="preview"
           data-src="{{item.res}}"
           src="{{item.res}}"></image>
</view>
